{
  "name": "sdn-pipeline",
  "version": "2.1.0",
  "description": "data pipeline for SingularData.io",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "test-ci": "NODE_ENV=ci npm run test",
    "build": "tsc --outDir dist",
    "precommit": "npm run build && npm run test && lint-staged",
    "deploy": "npm run build && npm test && SLS_DEBUG=* sls deploy -v",
    "deploy-ci": "bash deploy.sh"
  },
  "lint-staged": {
    "*.{d.ts,ts,json,md}": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SingularData/data-pipeline.git"
  },
  "author": "Haoliang Yu <haholiang.yu@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/SingularData/data-pipeline",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/dotenv": "^4.0.2",
    "@types/fs-extra": "^5.0.0",
    "@types/mocha": "^2.2.48",
    "@types/sleep": "0.0.7",
    "aws-sdk-mock": "^1.7.0",
    "chai": "^4.1.2",
    "dotenv": "^5.0.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2",
    "serverless": "^1.26.0",
    "serverless-dotenv-plugin": "^1.0.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/elasticsearch": "^5.0.19",
    "@types/http-aws-es": "^1.1.3",
    "@types/lodash": "^4.14.100",
    "@types/node": "^9.4.0",
    "@types/node-fetch": "^1.6.7",
    "aws-sdk": "^2.188.0",
    "elasticsearch": "^14.1.0",
    "http-aws-es": "^4.0.0",
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "w3c-dcat": "^0.3.0"
  },
  "nyc": {
    "extension": [".ts"],
    "exclude": ["**/*.d.ts"],
    "reporter": ["text-summary", "html"],
    "all": true
  }
}
